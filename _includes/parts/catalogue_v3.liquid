					<script>
						function googleFormSubmit(event) {
							event.preventDefault(); // Prevent the default form submission behavior

							const form = document.getElementById('catalogueQuote');
							const formData = new FormData(form);

							const cartItemsField = document.getElementById('form-cart-items'); // Get a reference to the textarea
							const formSubmitButton = document.getElementById('form-submit-button'); // Get a reference to the textarea
							const formSubmitLabel = document.getElementById('form-submit-label'); // Get a reference to the textarea

							// Disable the submit button and change its text immediately
							// formSubmitButton.disabled = true;
							// formSubmitButton.textContent = 'Submitting...';

							fetch('https://docs.google.com/forms/d/e/1FAIpQLScSdYMiiBt5h2W1kwH4TU9BFmIb60c5fG4u_K0MAHnQcm4mtA/formResponse', {
								method: 'POST',
								body: formData,
								mode: 'no-cors' // Add this to handle potential CORS issues with Google Forms
							})
							.then(response => {

								// Explicitly clear the value of the non-tabbable textarea
								cartItemsField.innerHTML = '';
								// Clear other form fields (this should now work correctly for the tabbable fields)
								form.reset();

								formSubmitButton.disabled = true; 
								formSubmitButton.textContent = 'Thank You';
								formSubmitButton.className = 'button fill-blue block no-pointer no-cursor';

								formSubmitLabel.textContent = 'Our Team Will Get in Touch'
								formSubmitLabel.classList.remove('invisible');

								if (response.ok) {
									// Handle success
									console.log('Form submitted successfully!');
								} else {
									// Handle error
									console.error('Form submission failed.');
								}
							})
							.catch(error => {
								console.error('Error during form submission:', error);
							});
						}
					</script>
